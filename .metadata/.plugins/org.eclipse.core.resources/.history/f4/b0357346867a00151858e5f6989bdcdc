package hashIndex;

public class Main {
	static HashIndex hash;
	
	/**
	 * run testing through Hashindex.
	 * @param args
	 */
	public static void main(String[] args) {
		hash = new HashIndex();
		System.out.println("Test line of Hash Index!!");
		hash.put("1", "first");
		hash.put("2", "second");
		hash.put("3", "third");
		hash.put("4", "forth");
		hash.put("5", "fifth");
		hash.put("6", "sixth");
		hash.put("7", "seventh");
		hash.put("8", "eighth");
		
//		hash.remove("1");
//		hash.remove("2");
//		hash.remove("3");
//		hash.remove("4");
//		hash.remove("5");
//		hash.remove("6");
//		hash.remove("7");
//		hash.remove("8");
//		
		hash.saveContents();
		System.out.println("Job Done!");
	}


}
